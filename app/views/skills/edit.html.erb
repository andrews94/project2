<h2>Edit Skill</h2>
<%= form_for @skill do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>

 <%= f.label :Image %>
 <%= f.text_field :img_url %>

 <%= f.label :Video %>
 <%= f.text_field :video_url %>

 <%= f.label :description %>
 <%= f.text_field :description %>

 <%= f.submit %>

 <p><%= link_to 'Delete Skill', @skill,
           method: :delete,
           class: "delete",
           data: { confirm: 'Deleted!' } %></p>

 <% end %>
