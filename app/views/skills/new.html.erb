<h2>New Skill</h2>

<%= form_for @skill do |f| %>
 <%= f.label :name %>
 <%= f.text_field :name %>

 <%= f.label :Image %>
 <%= f.text_field :img_url %>

 <%= f.label :Video %>
 <%= f.text_field :video_url %>

 <%= f.label :description %>
 <%= f.text_field :description %>


 <%= f.select(:champion, options_for_select(@champion_champion))%>


 <%= f.submit %>
<% end %>
